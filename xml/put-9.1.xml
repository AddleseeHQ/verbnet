   <THEMROLES/>
   <FRAMES>
    <FRAME>
     <DESCRIPTION descriptionNumber="0.2" primary="NP V NP" secondary="Transitive; without Destination" xtag="0.2"/>
     <!-- Discourse has to provide specific Destination and configuration (Prep) of Theme wrt Destination-->
     <EXAMPLES>
      <EXAMPLE>
       I stashed the book.
      </EXAMPLE>
     </EXAMPLES>
     <SYNTAX>
