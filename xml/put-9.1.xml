     </SEMANTICS>
     <GL>
      <QUALIA>
       { formal = motion(e) AND Opposition(At(1, ?), At(1, -?))}
      </QUALIA>
      <EVENT_STRUCTURE>
       { var = e, initial_state = [{ objects.1.location = ? }], final_state = [{ objects.1.location = -? }] }
      </EVENT_STRUCTURE>
      <SUBCATEGORISATION>
       { var = 0, cat = NP, role = [u'Agent'] } / [u'+', u'animate'] 
	{ var = e, cat = VERB, role = [] } / [] 
	{ var = 1, cat = NP, role = [u'Theme'] } / [u'+', u'concrete']
      </SUBCATEGORISATION>
     </GL>
    </FRAME>
   </FRAMES>
   <SUBCLASSES/>
  </VNSUBCLASS>
  <VNSUBCLASS ID="put-9.1-2">
